<% provide(:title, "Log in") %>
<h1>Log in</h1>

<div class="row">
  <div class="col-md-6 col-md-offset-3" align="center">
    <%= form_for(:session, url: success_path) do |f| %>

      <%= f.label :email %>
      <%= f.email_field :email, class: 'form-control' %><br/><br/>

      <%= f.label :password %>
      <%= f.password_field :password, class: 'form-control' %><br/><br/>

      <%= f.submit "Log in", class: "btn btn-primary" %>
    <% end %>

  </div>
</div>
<%= link_to 'Click Here', display_path %><br/>